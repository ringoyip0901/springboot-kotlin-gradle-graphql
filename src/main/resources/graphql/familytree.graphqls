type Query {
    version: Version
    message: String!
    relayId: ID
    getPersonById(id: ID!): Person
    getPersonByName(name: String): Person
    getAllPeople(offset: Int!): [Person]!
}

type Version {
    current: String
    previous: String
}

type Person {
#    strangeID: ID,
    id: String!,
    cursor: String!,
    name: String!,
    image: String,
    relationships(type: String): [Relationship]!
}

